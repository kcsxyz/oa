 <select id="selectSchedule" parameterType="String" resultMap="BaseResultMap">
 	select a.*,b.* from t_user a,t_schedule b where a.uid=b.create_name and b.create_name=#{uid}
 </select>
 <select id="getScheduleList" resultMap="BaseResultMap">
 	select 	
 		b.* 
 	from 
 		t_schedule b 
 	where
 		b.create_name=#{arg0}
 		<if test="arg1 !=null ">
 		and b.title  like CONCAT('%',#{arg1},'%')
 		</if>
 		<if test="arg2 !=null ">
 		and	b.start_time &gt;=#{arg2}
 		</if>
 		<if test="arg3 !=null ">
 		 and b.end_time &lt;=#{arg3}
 		</if>
 </select>
