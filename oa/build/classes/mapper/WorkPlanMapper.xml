  <select id="selectByParams"  resultMap="BaseResultMap"
       parameterType="map">
        SELECT
            *
        FROM
           t_work_plan wp left join t_user u on wp.create_name=u.uid
         <!--  t_work_plan -->
            <where>
            <if test=" param.Info!= null and  param.Info!= ''">
